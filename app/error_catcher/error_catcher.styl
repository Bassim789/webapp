.error_catcher_stacks
	padding 3px 0px

.error_catcher_stacks
	div
		display inline-block
		padding 0px 5px
		word-break break-all

.error_catcher_box
	width 100%
	max-width 800px
	margin 80px auto
	text-align center

#error_catcher_box
	display none
	position absolute
	top 0
	left 0
	right 0
	bottom 0
	background rgba(255, 255, 255, 0.9)
